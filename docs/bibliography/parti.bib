  shorttitle = {Chinese Temple Networks in Southeast Asia},
  author = {Yan, Yingwei and Dean, Kenneth and Feng, Chen-Chieh and Hue, Guan Thye and Koh, Khee-heong and Kong, Lily and Ong, Chang Woei and Tay, Arthur and Wang, Yi-chen and Xue, Yiran},
