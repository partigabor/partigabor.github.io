@article{dong_2021_directionality,
  title = {Directionality of {{Atmospheric Water}} in {{Chinese}}: {{A Lexical Semantic Study Based}} on {{Linguistic Ontology}}},
  author = {Dong, Sicong and Yang, Yike and Ren, He and Huang, Chu-Ren},
