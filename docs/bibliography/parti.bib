@article{breslin_1993_psychophysical,
  title = {Psychophysical {{Evidence That Oral Astringency Is}} a {{Tactile Sensation}}},
  author = {Breslin, P. A. S. and Gilmore, M. M. and Beauchamp, G. K. and Green, B. G.},
