@article{yang_2017_letting,
  title = {Letting the {{Troops Loose}}: {{Pillage}}, {{Massacres}}, and {{Enslavement}} in {{Early Tang Warfare}}},
  shorttitle = {Letting the {{Troops Loose}}},
  author = {Yang, Shao-yun},
