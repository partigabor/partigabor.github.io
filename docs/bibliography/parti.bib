  title = {A Dissenting Voice: The Clash of Trade and Warfare in {{Giovanni}} Da {{Empoli}}'s Account of His Second Voyage to {{Portuguese Asia}}},
  shorttitle = {A Dissenting Voice},
  author = {Salonia, Matteo},
  date = {2021-08},
